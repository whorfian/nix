super + t
  kitty
super + e
  kitty -e nvim
super + b
  google-chrome-stable
super + d
  discord
super + r
  rofi -show run  
super + w
  rofi -show window
super + {_,shift + }q
  bspc node -{c,k}
super + f
  bspc node --state ~fullscreen
super + Escape
  pkill -usr1 -x sxhkd && bspc wm -r
super + n 
  nrs
super + s
  maim --select | xclip -selection clipboard -target image/png
super + Tab
	bspc node -f last
super + {F11,F12}
  brightnessctl s 10{-,+}%
super + alt + Escape
  systemctl suspend



