super + t
  $TERMINAL
super + e
  $TERMINAL -e $EDITOR
super + b
  $BROWSER
super + d
  discord
super + r
  rofi -show run  
super + w
  rofi -show window
super + {_,shift + }q
  bspc node -{c,k}
super + Escape
  pkill -usr1 -x sxhkd && bspc wm -r
super + s
  maim --select | xclip -selection clipboard -target image/png
super + Tab
	bspc node -f last
super + alt + Escape
  systemctl suspend
super + {_,shift + }{1-9,0}
  bspc {desktop --focus,node --to-desktop} {1-9,10}

