super + p
  wmctrl -a "taigen" || kitty -e "abduco -A taigen nvim ~/taigen" --title "taigen"

super + t
  wmctrl -a "[[[kitty]]]" || kitty --title "[[[kitty]]]"
super + e
  wmctrl -a "Visual Studio Code" || code
  #$TERMINAL -e $EDITOR
super + b
  wmctrl -a "Google Chrome" || google-chrome-stable
super + d
  wmctrl -a "Discord" || discord

super + r
  rofi -show run  
super + w
  rofi -show window
super + {_,shift + }q
  bspc node -{c,k}
super + Escape
  pkill -usr1 -x sxhkd && bspc wm -r
super + s
  maim --select | xclip -selection clipboard -target image/png
super + Tab
	bspc node -f last
super + alt + Escape
  systemctl suspend
super + shift + r
  reboot
super + {_,shift + }{1-9,0}
  bspc {desktop --focus,node --to-desktop} {1-9,10}




